{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python",
            "config": {
                "pip": ["requirements.txt"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ]
}